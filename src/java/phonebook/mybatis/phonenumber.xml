<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
  
<mapper namespace="phonenumber"> 
 
    <resultMap id = "result" type = "PhoneNumber"> 
       <result property = "id" column = "id"/> 
       <result property = "phoneNum" column = "phoneNum"/> 
    </resultMap> 
 
    <select id = "selectAll" resultMap = "result"> 
       SELECT * FROM phonenumber;  
    </select> 

    <select id = "selectById" parameterType = "int" resultMap = "result"> 
       SELECT * FROM phonenumber WHERE id = #{id}; 
    </select> 

    <update id = "update" parameterType = "PhoneNumber"> 
       UPDATE phonenumber SET 
        phoneNum = #{phoneNum}, 
        WHERE id = #{id}; 
    </update> 
  
    <delete id = "deleteById" parameterType = "int"> 
       DELETE from phonenumber WHERE id = #{id}; 
    </delete> 

    <insert id = "insert" parameterType = "PhoneNumber"> 
       INSERT INTO PhoneNumber (phoneNum)  
       VALUES (#{phoneNum});     
    </insert>
</mapper> 